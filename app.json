{
  "expo": {
    "name": "Taxi Taxi",
    "slug": "Taxi_Taxi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/taxitaxi-logo.png",
    "scheme": "taxitaxi",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "20541a7f-745f-446d-a3ca-ab7fc03f6a25"
      }
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDqaqO6czuq1cTzTetNJU6yhnQP2b6_k8U"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby rides and navigation.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location for continuous tracking during rides.",
        "NSLocationAlwaysUsageDescription": "We need your location for continuous tracking during rides."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDqaqO6czuq1cTzTetNJU6yhnQP2b6_k8U"
        }
      },
      "package": "com.adroitfirm.taxitaxi"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "owner": "adroitfirm"
  }
}
